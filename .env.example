# Midscene 配置
MIDSCENE_USE_QWEN3_VL=true

# OpenAI 配置
OPENAI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
OPENAI_API_KEY=your_api_key_here

# 模型配置
MIDSCENE_MODEL_NAME=qwen3-vl-plus

# 服务器配置
PORT=3333
HOST=0.0.0.0

# 环境配置
NODE_ENV=production

# 日志配置
# 日志级别 (默认: info)
# 可选值: error, warn, info, http, verbose, debug, silly
LOG_LEVEL=info

# 日志目录 (默认: ./logs)
LOG_DIR=./logs

# 日志文件最大大小 (默认: 20m)
# 支持格式: 10M, 100K, 1G 等
LOG_MAX_SIZE=20m

# 日志文件保留时间 (默认: 14d)
# 支持格式: 7d, 30d, 1m 等
LOG_MAX_FILES=14d

# 日志文件日期格式 (默认: YYYY-MM-DD)
LOG_DATE_PATTERN=YYYY-MM-DD